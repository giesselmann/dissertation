  keywords = {DNA Methylation, Humans, Sequence Analysis, DNA, Sequence Alignment, Cytosine, Algorithms, Nanopores, High-Throughput Nucleotide Sequencing, 5-Methylcytosine, Markov Chains, Epigenomics},
  language = {eng},
  pmcid    = {PMC4553831},

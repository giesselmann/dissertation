  language  = {en},
  url       = {https://www.biorxiv.org/content/early/2018/09/26/426858},
  urldate   = {2018-11-02},
}

@Article{Mueller2018,
  author    = {Mueller, Carolin A. and Boemo, Michael A. and Spingardi, Paolo and Kessler, Benedikt and Kriaucionis, Skirmantas and Simpson, Jared T. and Nieduszynski, Conrad A.},
  title     = {Capturing the dynamics of genome replication on individual ultra-long nanopore sequence reads},
  journal   = {bioRxiv},
  year      = {2018},
  pages     = {442814},
  month     = oct,
  abstract  = {The replication of eukaryotic genomes is highly stochastic, making it difficult to determine the replication dynamics of individual molecules with existing methods. We now report a sequencing method for the measurement of replication fork movement on single molecules by Detecting Nucleotide Analogue signal currents on extremely long nanopore traces (D-NAscent). Using this method, we detect BrdU incorporated by Saccharomyces cerevisiae to reveal, at a genomic scale and on single molecules, the DNA sequences replicated during a pulse labelling period. Under conditions of limiting BrdU concentration, D-NAscent detects the differences in BrdU incorporation frequency across individual molecules to reveal the location of active replication origins, fork direction, termination sites, and fork pausing/stalling events. We used sequencing reads of 20-160 kb, to generate the first whole genome single-molecule map of DNA replication dynamics and discover a new class of low frequency stochastic origins in budding yeast.},
  copyright = {© 2018, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution 4.0 International), CC BY 4.0, as described at http://creativecommons.org/licenses/by/4.0/},
  doi       = {10.1101/442814},
  file      = {Full Text PDF:D\:\\owncloud\\Promotion\\Zotero\\storage\\Z89WHET9\\Mueller et al. - 2018 - Capturing the dynamics of genome replication on in.pdf:application/pdf;Snapshot:D\:\\owncloud\\Promotion\\Zotero\\storage\\ILFI243N\\442814.html:text/html},
  language  = {en},
  url       = {https://www.biorxiv.org/content/early/2018/10/24/442814},

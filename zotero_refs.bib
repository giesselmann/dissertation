@Article{Johnson2017,
  author   = {Johnson, Sarah S. and Zaikova, Elena and Goerlitz, David S. and Bai, Yu and Tighe, Scott W.},
  title    = {Real-{Time} {DNA} {Sequencing} in the {Antarctic} {Dry} {Valleys} {Using} the {Oxford} {Nanopore} {Sequencer}},

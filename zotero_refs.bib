  shorttitle = {{STRetch}},
}

@Article{Liu2017,
  author   = {Liu, Qian and Zhang, Peng and Wang, Depeng and Gu, Weihong and Wang, Kai},
  title    = {Interrogating the "unsequenceable" genomic trinucleotide repeat disorders by long-read sequencing},
  journal  = {Genome Medicine},

  keywords   = {Humans, Sequence Analysis, DNA, Software, DNA, Polymerase Chain Reaction, Escherichia coli},
  language   = {eng},
  pmcid      = {PMC5408768},
  pmid       = {27614348},
  shorttitle = {Nanocall},
}


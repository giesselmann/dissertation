@Article{Hennion2018,
  author    = {Hennion, Magali and Arbona, Jean-Michel and Cruaud, Corinne and Proux, Florence and Tallec, Benoît Le and Novikova, Elizaveta and Engelen, Stéfan and Lemainque, Arnaud and Audit, Benjamin and Hyrien, Olivier},
  title     = {Mapping {DNA} replication with nanopore sequencing},
  journal   = {bioRxiv},
  year      = {2018},
  pages     = {426858},
  month     = sep,
  abstract  = {We have harnessed nanopore sequencing to study DNA replication genome-wide at the single-molecule level. Using in vitro prepared DNA substrates, we characterized the effect of bromodeoxyuridine (BrdU) substitution for thymidine on the MinION nanopore electrical signal. Using a neural-network basecaller trained on yeast DNA containing either BrdU or thymidine, we identified BrdU-labelled tracts in yeast cells synchronously entering S phase in the presence of hydroxyurea and BrdU. As expected, the BrdU-labelled tracts coincided with previously identified early-firing, but not late-firing, replication origins. These results open the way to high-throughput, high-resolution, single-molecule analysis of DNA replication in many experimental systems.},
  copyright = {© 2018, Posted by Cold Spring Harbor Laboratory. The copyright holder for this pre-print is the author. All rights reserved. The material may not be redistributed, re-used or adapted without the author's permission.},
  doi       = {10.1101/426858},

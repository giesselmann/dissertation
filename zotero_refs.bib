@Article{Dijk2018,
  author   = {van Dijk, Erwin L. and Jaszczyszyn, Yan and Naquin, Delphine and Thermes, Claude},
  title    = {The {Third} {Revolution} in {Sequencing} {Technology}},
  journal  = {Trends in Genetics},
  year     = {2018},
  volume   = {34},
  number   = {9},

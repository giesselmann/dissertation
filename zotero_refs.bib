@Article{Ebbert2018,
  author     = {Ebbert, Mark T. W. and Farrugia, Stefan and Sens, Jonathon and Jansen-West, Karen and Gendron, Tania and Prudencio, Mercedes and DeJesus-Hernandez, Mariely and Brown, Patricia and Dickson, Dennis and Blitterswijk, Marka van and Rademakers, Rosa and Petrucelli, Leonard and Fryer, John D.},
  title      = {Long-read sequencing across the {C9orf72} '{GGGGCC}' repeat expansion: implications for genetic discovery efforts in human disease},
  journal    = {bioRxiv},
  year       = {2018},
  pages      = {176651},
  month      = feb,
  abstract   = {Many neurodegenerative diseases are caused by nucleotide repeat expansions, but most expansions, like the C9orf72 'GGGGCC' (G4C2) repeat that causes approximately 5-7\% of all amyotrophic lateral sclerosis (ALS) and frontotemporal dementia (FTD) cases, are too long to fully sequence using short-read sequencing technologies. It is unclear whether long-read sequencing technologies can traverse these long, challenging repeat expansions. Here, we demonstrate that two long-read sequencing technologies, Pacific Biosciences' (PacBio) and Oxford Nanopore Technologies', can sequence through disease-causing repeats cloned into plasmids, including the FTD/ALS-causing G4C2 repeat expansion. We also report the first long-read sequencing data characterizing the C9orf72 G4C2 repeat expansion at the nucleotide level in a symptomatic expansion carrier. Our findings indicate that long-read sequencing is well suited to characterizing known disease-causing repeat expansions, and for discovering new disease-causing or disease-modifying repeat expansions that have gone undetected with conventional short-read sequencing. Larger and deeper long-read sequencing studies in C9orf72 expansion carriers will be important to determine heterogeneity and whether the repeats are interrupted by non-G4C2 content, potentially mitigating or modifying the disease course or age of onset, as interruptions are known to do in other repeat-expansion disorders. These results have broad implications across all diseases where the genetic etiology remains unclear.},
  copyright  = {© 2018, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NonCommercial-NoDerivs 4.0 International), CC BY-NC-ND 4.0, as described at http://creativecommons.org/licenses/by-nc-nd/4.0/},
  doi        = {10.1101/176651},
  file       = {Full Text PDF:D\:\\owncloud\\Promotion\\Zotero\\storage\\NN9B2KBN\\Ebbert et al. - 2018 - Long-read sequencing across the C9orf72 'GGGGCC' r.pdf:application/pdf;Snapshot:D\:\\owncloud\\Promotion\\Zotero\\storage\\RCDF3Q78\\176651.html:text/html},
  language   = {en},
  shorttitle = {Long-read sequencing across the {C9orf72} '{GGGGCC}' repeat expansion},
  url        = {https://www.biorxiv.org/content/early/2018/02/10/176651},
  urldate    = {2018-02-13},
}

@Article{Daron2017,
  author     = {Daron, Josquin and Slotkin, R. Keith},
  title      = {{EpiTEome}: {Simultaneous} detection of transposable element insertion sites and their {DNA} methylation levels},
  journal    = {Genome Biology},
  year       = {2017},
  volume     = {18},
  pages      = {91},
  month      = may,
  issn       = {1474-760X},
  abstract   = {The genome-wide investigation of DNA methylation levels has been limited to reference transposable element positions. The methylation analysis of non-reference and mobile transposable elements has only recently been performed, but required both genome resequencing and MethylC-seq datasets. We have created epiTEome, a program that detects both new transposable element insertion sites and their methylation states from a single MethylC-seq dataset. EpiTEome outperforms other split-read insertion site detection programs, even while functioning on bisulfite-converted reads. EpiTEome characterizes the previously discarded fraction of DNA methylation at sites of new insertions, enabling future investigation into the epigenetic regulation of non-reference and transposed elements.},
  annote     = {Pages 91 in PDF},
  doi        = {10.1186/s13059-017-1232-0},
  file       = {Full Text PDF:D\:\\owncloud\\Promotion\\Zotero\\storage\\X7I2JUI7\\Daron und Slotkin - 2017 - EpiTEome Simultaneous detection of transposable e.pdf:application/pdf;Snapshot:D\:\\owncloud\\Promotion\\Zotero\\storage\\869HA537\\s13059-017-1232-0.html:text/html},
  keywords   = {Bioinformatics, Bisulfite, Insertion site, MethylC-seq, Methylome, Split reads, Transposable elements},
  shorttitle = {{EpiTEome}},
  url        = {https://doi.org/10.1186/s13059-017-1232-0},
  urldate    = {2018-02-20},
}

@Article{Trizzino2018,
  author    = {Trizzino, Marco and Kapusta, Aurelie and Brown, Christopher},
  title     = {Transposable elements generate regulatory novelty in a tissue specific fashion},
  journal   = {bioRxiv},
  year      = {2018},
  pages     = {268771},
  month     = feb,
  abstract  = {Transposable elements (TE) are an important source of evolutionary novelty in gene regulation. However, the mechanisms by which TEs contribute to gene expression are largely uncharacterized. Here, we leverage Roadmap and GTEx data to investigate the dynamics of TE recruitment in 24 tissues. We find 112 human TE types enriched in active regions of the genome across tissues. SINEs and DNA transposons are the most frequently enriched classes, while LTRs are often co-opted in a tissue specific manner. We report across-tissue variability in TE enrichment in active regions. Genes with consistent expression across tissues are less likely to be associated to TE insertions. TE repression similarly follows tissue specific patterns, and LTRs are the most abundant class in repressed regions. Different TE classes are preferentially silenced in different ways: LTRs and LINEs are overrepresented in regions marked by H3K9me3, while other TEs are preferentially repressed by H3K27me3. Young TEs are typically enriched in repressed regions and depleted in active regions. We detect multiple instances of TEs with tissue specific regulatory activity. Such TEs provide binding sites for transcription factors that are master regulators for the given tissue. These tissue specific activated TEs are enriched in intronic enhancers, and significantly affect the expression of the associated genes. Finally we show that SVAs can act as transcriptional activators or repressors in a tissue specific context. We provide an integrated overview of the contribution of TEs to human gene regulation. Expanding previous analyses, we demonstrate that TEs generate regulatory novelty in a tissue specific fashion.},
  copyright = {© 2018, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution-NonCommercial 4.0 International), CC BY-NC 4.0, as described at http://creativecommons.org/licenses/by-nc/4.0/},
  doi       = {10.1101/268771},
  file      = {Full Text PDF:D\:\\owncloud\\Promotion\\Zotero\\storage\\J86LRT8L\\Trizzino et al. - 2018 - Transposable elements generate regulatory novelty .pdf:application/pdf;Snapshot:D\:\\owncloud\\Promotion\\Zotero\\storage\\IPYUALN3\\268771.html:text/html},
  language  = {en},
  url       = {https://www.biorxiv.org/content/early/2018/02/20/268771},
  urldate   = {2018-02-22},
}

@Article{Kvikstad2018,
  author   = {Kvikstad, Erika M. and Piazza, Paolo and Taylor, Jenny C. and Lunter, Gerton},
  title    = {A high throughput screen for active human transposable elements},
  journal  = {BMC genomics},
  year     = {2018},
  volume   = {19},
  number   = {1},
  pages    = {115},
  issn     = {1471-2164},
  abstract = {BACKGROUND: Transposable elements (TEs) are mobile genetic sequences that randomly propagate within their host's genome. This mobility has the potential to affect gene transcription and cause disease. However, TEs are technically challenging to identify, which complicates efforts to assess the impact of TE insertions on disease. Here we present a targeted sequencing protocol and computational pipeline to identify polymorphic and novel TE insertions using next-generation sequencing: TE-NGS. The method simultaneously targets the three subfamilies that are responsible for the majority of recent TE activity (L1HS, AluYa5/8, and AluYb8/9) thereby obviating the need for multiple experiments and reducing the amount of input material required.
RESULTS: Here we describe the laboratory protocol and detection algorithm, and a benchmark experiment for the reference genome NA12878. We demonstrate a substantial enrichment for on-target fragments, and high sensitivity and precision to both reference and NA12878-specific insertions. We report 17 previously unreported loci for this individual which are supported by orthogonal long-read evidence, and we identify 1470 polymorphic and novel TEs in 12 additional samples that were previously undocumented in databases of insertion polymorphisms.
CONCLUSIONS: We anticipate that future applications of TE-NGS alongside exome sequencing of patients with sporadic disease will reduce the number of unresolved cases, and improve estimates of the contribution of TEs to human genetic disease.},
  doi      = {10.1186/s12864-018-4485-4},
  keywords = {Bioinformatics, Transposable elements, Alu, LINE1, Next generation sequencing, Polymorphism},
  language = {eng},
  pmcid    = {PMC5796560},
  pmid     = {29390960},
}


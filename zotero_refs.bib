  keywords   = {Neural networks, Sequence alignment, Genomics, Klebsiella pneumoniae, Hidden Markov models, Open source software, Probability distribution, Recurrent neural networks},
  language   = {en},
  shorttitle = {{DeepNano}},
  url        = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0178751},

@Article{Liu2020,
  author   = {Liu, Yibin and Cheng, Jingfei and Siejka-Zielińska, Paulina and Weldon, Carika and Roberts, Hannah and Lopopolo, Maria and Magri, Andrea and D’Arienzo, Valentina and Harris, James M. and McKeating, Jane A. and Song, Chun-Xiao},
  title    = {Accurate targeted long-read {DNA} methylation and hydroxymethylation sequencing with {TAPS}},
  journal  = {Genome Biology},
  year     = {2020},
  volume   = {21},
  number   = {1},
  pages    = {54},
  month    = mar,
  issn     = {1474-760X},
  abstract = {We present long-read Tet-assisted pyridine borane sequencing (lrTAPS) for targeted base-resolution sequencing of DNA methylation and hydroxymethylation in regions up to 10 kb from nanogram-level input. Compatible with both Oxford Nanopore and PacBio Single-Molecule Real-Time (SMRT) sequencing, lrTAPS detects methylation with accuracy comparable to short-read Illumina sequencing but with long-range epigenetic phasing. We applied lrTAPS to sequence difficult-to-map regions in mouse embryonic stem cells and to identify distinct methylation events in the integrated hepatitis B virus genome.},
  doi      = {10.1186/s13059-020-01969-6},
  file     = {Full Text PDF:D\:\\owncloud\\Promotion\\Zotero\\storage\\Z3V89R7A\\Liu et al. - 2020 - Accurate targeted long-read DNA methylation and hy.pdf:application/pdf;Snapshot:D\:\\owncloud\\Promotion\\Zotero\\storage\\LL3FD35J\\s13059-020-01969-6.html:text/html},
  keywords = {DNA methylation, 5-Methylcytosine, Long-read sequencing, Bisulfite-free, Epigenetic phasing},
  url      = {https://doi.org/10.1186/s13059-020-01969-6},
  urldate  = {2021-02-04},
}

@Article{Schreiber2013,
  author   = {Schreiber, Jacob and Wescoe, Zachary L. and Abu-Shumays, Robin and Vivian, John T. and Baatar, Baldandorj and Karplus, Kevin and Akeson, Mark},
  title    = {Error rates for nanopore discrimination among cytosine, methylcytosine, and hydroxymethylcytosine along individual {DNA} strands},
  journal  = {Proceedings of the National Academy of Sciences},
  year     = {2013},
  volume   = {110},
  number   = {47},
  pages    = {18910--18915},
  month    = nov,
  issn     = {0027-8424, 1091-6490},
  note     = {Publisher: National Academy of Sciences Section: Biological Sciences},
  abstract = {Cytosine, 5-methylcytosine, and 5-hydroxymethylcytosine were identified during translocation of single DNA template strands through a modified Mycobacterium smegmatis porin A (M2MspA) nanopore under control of phi29 DNA polymerase. This identification was based on three consecutive ionic current states that correspond to passage of modified or unmodified CG dinucleotides and their immediate neighbors through the nanopore limiting aperture. To establish quality scores for these calls, we examined ∼3,300 translocation events for 48 distinct DNA constructs. Each experiment analyzed a mixture of cytosine-, 5-methylcytosine–, and 5-hydroxymethylcytosine–bearing DNA strands that contained a marker that independently established the correct cytosine methylation status at the target CG of each molecule tested. To calculate error rates for these calls, we established decision boundaries using a variety of machine-learning methods. These error rates depended upon the identity of the bases immediately 5′ and 3′ of the targeted CG dinucleotide, and ranged from 1.7\% to 12.2\% for a single-pass read. We estimate that Q40 values (0.01\% error rates) for methylation status calls could be achieved by reading single molecules 5–19 times depending upon sequence context.},
  doi      = {10.1073/pnas.1310615110},
  file     = {Full Text PDF:D\:\\owncloud\\Promotion\\Zotero\\storage\\X2GNFX3U\\Schreiber et al. - 2013 - Error rates for nanopore discrimination among cyto.pdf:application/pdf;Snapshot:D\:\\owncloud\\Promotion\\Zotero\\storage\\BMMCS5QM\\18910.html:text/html},
  keywords = {epigenetics, MspA},
  language = {en},
  pmid     = {24167260},
  url      = {https://www.pnas.org/content/110/47/18910},
  urldate  = {2021-02-04},
}


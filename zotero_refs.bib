@Article{Leggett2016a,
  author     = {Leggett, Richard M. and Heavens, Darren and Caccamo, Mario and Clark, Matthew D. and Davey, Robert P.},
  title      = {{NanoOK}: multi-reference alignment analysis of nanopore sequencing data, quality and error profiles},

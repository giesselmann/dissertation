  file         = {arXiv\:1712.07628 PDF:E\:\\owncloud\\Promotion\\Zotero\\storage\\XNP67DEB\\Keskar und Socher - 2017 - Improving Generalization Performance by Switching .pdf:application/pdf;arXiv.org Snapshot:E\:\\owncloud\\Promotion\\Zotero\\storage\\J9Q8U838\\1712.html:text/html},

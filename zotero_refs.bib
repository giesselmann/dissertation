  author       = {Bernhart, Stephan H. and Kretzmer, Helene and Holdt, Lesca M. and Jühling, Frank and Ammerpohl, Ole and Bergmann, Anke K. and Northoff, Bernd H. and Doose, Gero and Siebert, Reiner and Stadler, Peter F. and Hoffmann, Steve},
